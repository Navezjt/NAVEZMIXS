    "mobx-utils": "^6.0.4",
    "mongodb": "^3.6.8",
    "next": "10.2.2",
    "next-auth": "^3.23.3",
    "next-connect": "^0.10.1",
    "next-pwa": "^5.2.21",
    "nice-fetch": "^1.0.4",
    "node-internet-radio": "^0.2.1",
    "nookies": "^2.5.2",
    "notistack": "^1.0.9",
    "radio-browser-api": "^2.2.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
