{
  "name": "next-radio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "yarn generate && yarn lang && next build",
    "start": "next start",
    "analyze": "env ANALYZE=true npm run build",
    "gen:countries": "node scripts/generate-countries.js",
    "gen:languages": "node scripts/generate-languages.js",
    "gen:genres": "node scripts/generate-genres.js",
    "generate": "npm-run-all gen:*",
    "lang:extract": "NODE_ENV=development lingui extract",
    "lang:compile": "lingui compile",
    "lang": "yarn lang:extract && yarn lang:compile",
    "type:check": "tsc",
    "fix:src": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
