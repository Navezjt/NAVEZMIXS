    "lang:compile": "lingui compile",
    "lang": "yarn lang:extract && yarn lang:compile",
    "fix:src": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "postinstall": "patch-package"
